import("//build/ohos.gni")
import("//foundation/graphic/graphic_2d/ace_platforms.gni")
import("//foundation/graphic/graphic_2d/graphic_config.gni")

skia_root = "//third_party/skia"

external_icu_include_dirs = []

icu_include_dirs = [
  "$skia_root/third_party/externals/icu/source/common",
  "$skia_root/third_party/externals/icu/source/i18n",
  "$skia_root/third_party/externals/icu/source",
  "$skia_root/third_party/icu",
]

template("icu_config") {
  forward_variables_from(invoker, "*")

  config(target_name) {
    visibility = [ ":*" ]
    if (defined(config.use_external_icu)) {
      include_dirs = external_icu_include_dirs
    } else {
      include_dirs = icu_include_dirs
    }
  }
}

foreach(item, ace_platforms) {
  icu_config("icu_config_" + item.name) {
    config = {
    }
    if (defined(item.config)) {
      config = item.config
    }
  }
}

config("icu_compile_flags") {
  cflags_cc = [
    "-std=c++14",
    "-fno-exceptions",

    # "-fvisibility-inlines-hidden",
    "-Os",
  ]

  cflags = [
    "-w",
    "-fstrict-aliasing",
    "-O3",
    "-fdata-sections",
    "-ffunction-sections",
    "-gline-tables-only",
    "-funwind-tables",
    "-Wno-unused-parameter",

    # "-fvisibility=hidden",
    "-Os",
  ]
  if (use_mingw_win) {
    cflags_cc += [ "-DWINVER=0x0601" ]
  }
}

template("rosen_icu_make_data_assembly") {
  forward_variables_from(invoker, "*")

  action(target_name) {
    if (platform == "mingw") {
      platform = "windows"
    }
    script =
        "$skia_root/third_party/externals/icu/scripts/make_data_assembly.py"
    if (platform == "windows" || platform == "mac") {
      inputs = [ "$skia_root/third_party/externals/icu/common/icudtl.dat" ]
    } else {
      inputs = [ "$skia_root/third_party/externals/icu/flutter/icudtl.dat" ]
    }
    outputs = [ "$root_out_dir/rosen_icu_$platform/icudtl_dat.S" ]
    args = [
      rebase_path(inputs[0], root_build_dir),
      rebase_path(outputs[0], root_build_dir),
    ]
    if (platform == "mac") {
      args += [ "--mac" ]
    }
    if (defined(use_new_skia) && use_new_skia) {
      if (platform == "windows") {
        args += [ "--win" ]
      }
    }
  }
}

foreach(item, ace_platforms) {
  rosen_icu_make_data_assembly("rosen_icu_make_data_assembly_" + item.name) {
    platform = item.name
  }
}

