import("//build/ohos.gni")
import("//foundation/graphic/graphic_2d/graphic_config.gni")

group("test") {
  testonly = true

  deps = [ "unittest:unittest" ]
}

ohos_executable("graphics_effect_exe") {
  branch_protector_ret = "pac_ret"
  sanitize = {
    cfi = true
    cfi_cross_dso = true
    debug = false
  }

  sources = [ "main.cpp" ]

  include_dirs = []

  deps = [ "$graphic_2d_root/rosen/modules/graphics_effect:libgraphics_effect" ]

  part_name = "graphic_2d"
  subsystem_name = "graphic"
}
